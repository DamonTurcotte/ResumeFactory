            "./assets/fonts/CrimsonPro/CrimsonPro-Italic.ttf",
            "./assets/fonts/CrimsonPro/CrimsonPro-Medium.ttf",
            "./assets/fonts/CrimsonPro/CrimsonPro-MediumItalic.ttf",
            "./assets/fonts/CrimsonPro/CrimsonPro-SemiBold.ttf",
            "./assets/fonts/CrimsonPro/CrimsonPro-SemiBoldItalic.ttf",
            "./assets/fonts/DMSans/DMSans-Regular.ttf",
            "./assets/fonts/DMSans/DMSans-Italic.ttf",
            "./assets/fonts/DMSans/DMSans-Medium.ttf",
            "./assets/fonts/DMSans/DMSans-MediumItalic.ttf",
            "./assets/fonts/DMSans/DMSans-SemiBold.ttf",
            "./assets/fonts/DMSans/DMSans-SemiBoldItalic.ttf",
            "./assets/fonts/Genos/Genos-Regular.ttf",
            "./assets/fonts/Genos/Genos-Italic.ttf",
            "./assets/fonts/Genos/Genos-Medium.ttf",
            "./assets/fonts/Genos/Genos-MediumItalic.ttf",
