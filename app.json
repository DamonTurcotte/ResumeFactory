{
  "expo": {
    "name": "Resume Factory",
    "slug": "ResumeFactory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F378B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "tech.turcotte.resumefactory"
    },
    "android": {
      "versionCode": 1,
      "package": "tech.turcotte.resumefactory",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4F378B"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "A dynamic resume constructor built with React Native and Expo. This application is built to help users to easily customize the design of their resume while storing multiple profiles to optimize for different job positions. Ad-free and fully offline, Resume Factory makes resume construction as quick and painless as possible, with seemless navigation and a simple and intuitive interface to craft your ticket to your next job position with its selection of professional and modern template designs.",
    "githubUrl": "https://github.com/Altagrave-Git/ResumeFactory",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "primaryColor": "#6750A4",
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Orbitron/Orbitron-Regular.ttf",
            "./assets/fonts/Orbitron/Orbitron-Medium.ttf",
            "./assets/fonts/Orbitron/Orbitron-SemiBold.ttf",
